config TARGET_LINUX_GIT_TREEISH
	string
	prompt "treeish of linux git repository"
	depends on TARGET_r6200v2
	default "Please Input a Valid Treeish"
	help
           Input the treeish of linux git repository which you want to build

config TARGET_LINUX_GIT_REPOSITORY
	string
	prompt "name of linux repository"
	depends on TARGET_r6200v2
	default "linux.git"
	help
	  input the name of linux repository which you want to build

config TARGET_LINUX_GIT_URL
	string
	prompt "url of linux repository"
	depends on TARGET_r6200v2
	default "ssh://dniserver/home/jonathan/linux.git"
	help
	  input the url of linux repository which you want to build

