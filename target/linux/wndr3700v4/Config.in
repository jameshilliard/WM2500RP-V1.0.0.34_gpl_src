config TARGET_LINUX_GIT_TREEISH
	string
	prompt "treeish of linux git repository"
	depends on TARGET_wndr3700v4
	default "linux-v2.6.31-dni13"
	help
           Input the treeish of linux git repository which you want to build

config TARGET_LINUX_GIT_REPOSITORY
	string
	prompt "name of linux git repository"
	depends on TARGET_wndr3700v4
	default "linux-2.6.git"
	help
	   Input the name of linux git repository which you want to build

config TARGET_LINUX_GIT_URL
	string
	prompt "URL of linux git repository"
	depends on TARGET_wndr3700v4
	default "ssh://dniserver/scm/dnigit/taipei-clones/linux-2.6.git"
	help
	   Input the url of linux git repository which you want to build

