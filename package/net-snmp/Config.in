config NET_SNMP_GIT_TREEISH
	string
	prompt "treeish of net-snmp git repository"
	default "net-snmp-5.0.9-dni7"
	help
           Input the treeish of net-snmp git repository which you want to build

config NET_SNMP_GIT_REPOSITORY
	string
	prompt "name of net-snmp git repository"
	default "net-snmp.git"
